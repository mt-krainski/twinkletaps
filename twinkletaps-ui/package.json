{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:testing": "NUXT_PUBLIC_ENV=testing nuxt dev",
    "start": "npm run dev",
    "start:built": "node .output/server/index.mjs",
    "cypress:e2e": "cypress run --e2e",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cypress:e2e",
    "test:e2e": "start-server-and-test 'NUXT_PUBLIC_ENV=testing npm run start:built' http://localhost:3000 cypress:e2e",
    "lint": "npx eslint --ignore-pattern **/.output/** --ignore-pattern **/.nuxt/** --ignore-pattern *.mjs --ignore-pattern *.cjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.38.0",
    "@eslint/migrate-config": "^1.6.0",
    "@mdi/font": "^7.4.47",
    "@nuxt/kit": "^4.1.3",
    "@pinia/nuxt": "^0.11.2",
    "add": "^2.0.6",
    "axios": "^1.12.2",
    "globals": "^16.4.0",
    "module-alias": "^2.2.3",
    "nuxt": "^3.17.7",
    "pinia": "^3.0.3",
    "sass": "^1.93.2",
    "vite-plugin-vuetify": "^2.1.2",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.3"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
