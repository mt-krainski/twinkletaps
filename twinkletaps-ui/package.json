    "cypress": "^13.14.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.28.0",
    "gts": "^5.3.1",
