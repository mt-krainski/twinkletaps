{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:testing": "NUXT_PUBLIC_ENV=testing nuxt dev",
    "start": "npm run dev",
    "start:built": "node .output/server/index.mjs",
    "cypress:e2e": "cypress run --e2e",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cypress:e2e",
    "test:e2e": "start-server-and-test 'NUXT_PUBLIC_ENV=testing npm run start:built' http://localhost:3000 cypress:e2e",
    "lint": "npx eslint --ignore-pattern **/.output/** --ignore-pattern **/.nuxt/** --ignore-pattern *.mjs --ignore-pattern *.cjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.23.0",
    "@eslint/migrate-config": "^1.4.1",
    "@mdi/font": "^7.4.47",
    "@nuxt/kit": "^3.17.0",
    "@pinia/nuxt": "^0.9.0",
    "add": "^2.0.6",
    "axios": "^1.9.0",
    "globals": "^16.0.0",
    "module-alias": "^2.2.3",
    "nuxt": "^3.16.2",
    "pinia": "^2.3.1",
    "sass": "^1.86.3",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.5.12",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.3.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "typescript": "^5.8.3"
