{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:testing": "NUXT_PUBLIC_ENV=testing nuxt dev",
    "start": "npm run dev",
    "start:built": "node .output/server/index.mjs",
    "cypress:e2e": "cypress run --e2e",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cypress:e2e",
    "test:e2e": "start-server-and-test 'NUXT_PUBLIC_ENV=testing npm run start:built' http://localhost:3000 cypress:e2e",
    "lint": "npx eslint --ignore-pattern **/.output/** --ignore-pattern **/.nuxt/** --ignore-pattern *.mjs --ignore-pattern *.cjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@eslint/migrate-config": "^1.3.1",
    "@mdi/font": "^7.4.47",
    "@nuxt/kit": "^3.13.0",
    "@pinia/nuxt": "^0.5.4",
    "add": "^2.0.6",
    "axios": "^1.7.7",
    "globals": "^15.11.0",
    "module-alias": "^2.2.3",
    "nuxt": "^3.13.2",
    "pinia": "^2.2.3",
    "sass": "^1.78.0",
    "vite-plugin-vuetify": "^2.0.4",
    "vue": "^3.5.12",
    "vue-router": "^4.4.3",
    "vuetify": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.9.0",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.15.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^4.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.28.0",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "typescript": "^5.6.2"
  }
