    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/kit": "^3.13.0",
    "@pinia/nuxt": "^0.5.4",
    "add": "^2.0.6",
    "axios": "^1.7.7",
    "module-alias": "^2.2.3",
    "nuxt": "^3.13.2",
    "pinia": "^2.2.3",
    "sass": "^1.78.0",
    "vite-plugin-vuetify": "^2.0.4",
    "vue": "^3.5.10",
    "vue-router": "^4.4.3",
    "vuetify": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
