    "gts:lint": "gts lint '**/*.ts' '**/*.vue'",
    "gts:clean": "gts clean '**/*.ts' '**/*.vue'",
    "gts:fix": "gts fix '**/*.ts' '**/*.vue'",
    "lint": "npm run gts:lint",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/kit": "^3.12.3",
    "@pinia/nuxt": "^0.5.2",
    "add": "^2.0.6",
    "axios": "^1.7.2",
    "module-alias": "^2.2.3",
    "nuxt": "^3.12.4",
    "pinia": "^2.1.7",
    "sass": "^1.77.8",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.35",
    "vue-router": "^4.3.2",
    "vuetify": "^3.6.14"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
